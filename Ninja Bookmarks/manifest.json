{
    "name": "Ninja Bookmarks",
    "description": "This Extension Helps You To Bookmark your pages easily and quickly",
    "version": "1.0",
    "manifest_version": 2,
    "icons": {
      "128": "icon_128.png"
    },
    "browser_action":{
        "default_popup":"popup.html"
    },
    "permissions": [
      "tabs","bookmarks","storage","http://localhost/*"
  ]
    }
